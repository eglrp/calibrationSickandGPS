function loss = lossfunction (se,datain)
       R= RPYtoR(se(1:3));
       t= [se(4);se(5);se(6)];
       RTl2c = [R , t;
            0 0 0 ,1];
        num_cal = size(datain,1);
        loss = 0;
        
        for i =1 :1 :num_cal
            
            
        end
        loss = loss/num_cal;




end